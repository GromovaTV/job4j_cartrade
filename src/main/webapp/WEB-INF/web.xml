<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>controller.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AboutServlet</servlet-name>
    <servlet-class>controller.AboutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PhotoUploadServlet</servlet-name>
    <servlet-class>controller.PhotoUploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchFilterServlet</servlet-name>
    <servlet-class>controller.SearchFilterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditServlet</servlet-name>
    <servlet-class>controller.EditServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PhotoDownloadServlet</servlet-name>
    <servlet-class>controller.PhotoDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/all.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AboutServlet</servlet-name>
    <url-pattern>/about.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PhotoUploadServlet</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchFilterServlet</servlet-name>
    <url-pattern>/searchfilter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditServlet</servlet-name>
    <url-pattern>/edit.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PhotoDownloadServlet</servlet-name>
    <url-pattern>/download.do</url-pattern>
  </servlet-mapping>
</web-app>
